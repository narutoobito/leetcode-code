class Solution {
public:
    bool lemonadeChange(vector<int>& bills) {
        vector<int>arr(4);
        for(auto i:bills){
            if(i!=5 and arr[0]==0)return false;
            if(i==10){
                arr[0]--;
            }
            if(i==20){
                if((arr[1]<1 or arr[0]<1) and arr[0]<3)return false;
                if(arr[1]>=1){
                    arr[1]--;arr[0]--;
                }
                else{
                    arr[0]-=3;
                }
            }
            arr[(i/10)]++;
            // cout<<arr[0]<<" "<<arr[1]<<" "<<arr[2]<<endl;
        }

        return true;
    }
};
